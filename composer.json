{
    "name": "php-junior/laravel-global-search",
    "description": "Search Package For Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel", "search", "global"
    ],
    "authors": [
        {
            "name": "Nyi Nyi Lwin",
            "email": "nyinyilwin1992@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "psr-4": {
            "PhpJunior\\LaravelGlobalSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpJunior\\LaravelGlobalSearch\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhpJunior\\LaravelGlobalSearch\\LaravelGlobalSearchProvider"
            ],
            "aliases": {
                "LaravelGlobalSearch": "PhpJunior\\LaravelGlobalSearch\\Facades\\LaravelGlobalSearch"
            }
        }
    }
}
